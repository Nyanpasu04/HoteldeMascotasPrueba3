@model IEnumerable<HoteldeMascotas.Models.Mascotas>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreMascota)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Raza)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Años)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Vacunas)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DueñosID)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreMascota)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Raza)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Años)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Vacunas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DueñosID)
            </td>
            
        </tr>
}
    </tbody>
</table>
